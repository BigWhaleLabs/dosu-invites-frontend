{
  "name": "dosu-invites-frontend",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "yarn prepare && vite",
    "build": "yarn prepare && cross-env NODE_ENV=production tsc && vite build",
    "pretty": "prettier --check src",
    "lint": "yarn pretty && eslint --max-warnings 0 --ext ts,tsx src",
    "postinstall": "yarn prepare",
    "prepare": "yarn generate-css-types",
    "generate-css-types": "tailwindcss-classnames -o src/classnames/tailwind.ts && prettier --write 'src/classnames/tailwind.ts'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@big-whale-labs/constants": "^0.1.29",
    "@big-whale-labs/dosu-invites-contract": "^0.0.12",
    "@big-whale-labs/frontend-utils": "^0.0.8",
    "@coinbase/wallet-sdk": "^3.5.2",
    "@walletconnect/web3-provider": "^1.8.0",
    "axios": "^0.27.2",
    "copy-to-clipboard": "^3.3.2",
    "envalid": "^7.3.1",
    "eth-rpc-errors": "^4.0.3",
    "ethers": "^5.7.0",
    "merkletreejs": "^0.2.32",
    "preact": "^10.11.0",
    "react-dom": "^18.2.0",
    "react-loadingg": "^1.7.2",
    "react-router-dom": "^6.3.0",
    "react-spring": "^9.5.4",
    "react-toastify": "^9.0.8",
    "valtio": "^1.7.0",
    "web3modal": "^1.9.9"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@preact/preset-vite": "^2.4.0",
    "@types/node": "^18.7.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.10",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-no-relative-import-paths": "^1.4.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-valtio": "^0.6.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "react-scripts": "^5.0.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-visualizer": "^5.8.1",
    "tailwindcss": "^3.1.8",
    "tailwindcss-classnames": "^3.0.6",
    "typescript": "^4.8.3",
    "vite": "^3.1.0",
    "vite-plugin-environment": "^1.1.2",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "packageManager": "yarn@3.2.3"
}
