{
  "name": "dosu-invites-frontend",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "yarn prepare && vite",
    "build": "yarn prepare && cross-env NODE_ENV=production tsc && vite build",
    "pretty": "prettier --check src",
    "lint": "yarn pretty && eslint --max-warnings 0 --ext ts,tsx src",
    "postinstall": "yarn prepare",
    "prepare": "yarn generate-css-types",
    "generate-css-types": "tailwindcss-classnames -o src/classnames/tailwind.ts && prettier --write 'src/classnames/tailwind.ts'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@big-whale-labs/constants": "^0.0.8",
    "@big-whale-labs/dosu-invites-contract": "^0.0.11",
    "@coinbase/wallet-sdk": "^3.1.0",
    "@walletconnect/web3-provider": "^1.7.8",
    "axios": "^0.27.2",
    "copy-to-clipboard": "^3.3.1",
    "envalid": "^7.3.1",
    "eth-rpc-errors": "^4.0.3",
    "ethers": "^5.6.6",
    "fortmatic": "^2.3.0",
    "merkletreejs": "^0.2.31",
    "preact": "^10.7.2",
    "react-dom": "^18.1.0",
    "react-loadingg": "^1.7.2",
    "react-router-dom": "^6.3.0",
    "react-spring": "^9.4.5",
    "react-toastify": "^9.0.1",
    "valtio": "^1.6.1",
    "web3modal": "^1.9.7"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@preact/preset-vite": "^2.2.0",
    "@types/node": "^17.0.35",
    "@types/react-dom": "^18.0.4",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-no-relative-import-paths": "^1.3.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-valtio": "^0.4.4",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "react-scripts": "^5.0.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "tailwindcss": "^3.0.24",
    "tailwindcss-classnames": "^3.0.5",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vite-plugin-environment": "^1.1.1",
    "vite-tsconfig-paths": "^3.4.1"
  },
  "packageManager": "yarn@3.1.1"
}
